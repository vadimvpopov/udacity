{
  "spec_dir": "spec",
  "spec_files": [
    "../**/tests/*[s]pec.ts",
    "../**/tests/**/*[sS]pec.js"
  ],
  "helpers": [
    "../**/tests/setup/**/*.js"
  ],
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "displayStacktrace": "all"
      }
    }
  ],
  "stopSpecOnExpectationFailure": false,
  "random": false
}
